{
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "all",
	"arrowParens": "always",
	"endOfLine": "auto",
	"useTabs": true,
	"printWidth": 180,
	"overrides": [
		{
			"files": "*.html",
			"options": {
				"printWidth": 150
			}
		},
		{
			"files": "*.module.ts",
			"options": {
				"printWidth": 50
			}
		}
	]
}
